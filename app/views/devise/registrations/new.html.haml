#cms-container
  .header
    .wraper
      .adminInfo
        %a{:href => "/users/sign_in", :style => "color:#666"} Login
      .logo
        = link_to image_tag('75.jpg',:size=>"50x70") ,root_path
      .clear
  %header.mainNav.boxLink
    %section.wraper
      %nav.topNav
        .clear
  .clear
  .bodyCnt
    .wraper
      .signin_inner
        #inner-main.signin
          %h2 Sign Up
          = form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f|
            = devise_error_messages!
            %div
              = f.label :name
              %br/
              = f.text_field :name
            %div
              = f.label :email
              %br/
              = f.email_field :email, autofocus: true
            %div
              = f.label :password
              %br/
              = f.password_field :password, autocomplete: "off"
            %div
              = f.label :password_confirmation
              %br/
              = f.password_field :password_confirmation, autocomplete: "off"
            %div
              = f.submit "Sign up"
  .clear
.clear